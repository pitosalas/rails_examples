<p id="notice"><%= notice %></p>

<h1>Ta</h1>

<table>
  <thead>
    <tr>
      <th>Student</th>
      <th>Course</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ta.each do |tum| %>
      <tr>
        <td><%= tum.student_id %></td>
        <td><%= tum.course_id %></td>
        <td><%= link_to 'Show', tum %></td>
        <td><%= link_to 'Edit', edit_tum_path(tum) %></td>
        <td><%= link_to 'Destroy', tum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tum', new_tum_path %>
